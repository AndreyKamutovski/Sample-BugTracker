{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/#",
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Attach",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:3000/app/project",
      "webRoot": "${workspaceFolder}"
    }
  ]
}